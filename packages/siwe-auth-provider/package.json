{
  "name": "use-rainbowkit-vue-siwe-auth-provider",
  "private": false,
  "version": "1.1.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir && npm run generate:types",
    "generate:types": "vue-tsc -p tsconfig.json && resolve-tspaths",
    "preview": "vite preview"
  },
  "main": "./dist/rainbowkit-vue-siwe-auth-adapter.js",
  "module": "./dist/rainbowkit-vue-siwe-auth-adapter.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/provider.d.ts",
      "import": "./dist/rainbowkit-vue-siwe-auth-adapter.js",
      "require": "./dist/rainbowkit-vue-siwe-auth-adapter.js"
    }
  },
  "peerDependencies": {
    "typescript": ">=5.0.4",
    "use-rainbowkit-vue": ">=0.4.1",
    "viem": ">=2.13.7",
    "vue": ">=3",
    "vue-auth3": ">=1.0.10"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "resolve-tspaths": "^0.8.19",
    "typescript": "^5.5.3",
    "use-rainbowkit-vue": "workspace:*",
    "viem": "^2.17.3",
    "vite": "^5.3.3",
    "vue-auth3": "^1.0.10",
    "vue-router": "^4.4.0",
    "vue-tsc": "^2.0.26"
  },
  "overrides": {
    "i18next": "^23.2.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xHoST/rainbowkit_vue.git"
  }
}
